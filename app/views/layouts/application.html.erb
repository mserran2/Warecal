	<script type="text/javascript">Cufon.replace('h1, h2, h3, h4, h5, h6, #nav', {hover:true});</script>
			<script type="text/javascript">
			window.addEventListener('DOMContentLoaded', function() {
   				$("body").queryLoader2({onComplete:fadeInPage, percentage:true});
			});
			function fadeInPage() {
				var temp = $("#controls_content").html();
				if(temp != null) {
					$("#controls").html(temp);
					$("#controls").slideDown();
				}
				$('#side_menu').show('slide');
				$('#header').show('clip');
				$('#content_scroll').show('blind');
				$("#side_menu ul li").mouseenter(function(){
					$(this).children('a').effect('shake',{distance: 5, times:3 },100)
			    })
			}
			/*
			$(function() {
